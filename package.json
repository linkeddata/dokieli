{
  "name": "dokieli",
  "version": "0.3.32",
  "description": "dokieli is a client-side editor for decentralised article publishing, annotations and social interactions",
  "main": "./src/dokieli.js",
  "scripts": {
    "build-dist": "webpack --progress --colors",
    "build": "npm run build-dist",
    "standard": "standard src/auth.js src/doc.js src/fetcher.js src/graph.js src/inbox.js src/simplerdf.js src/uri.js src/util.js ",
    "test": "npm run standard",
    "watch": "webpack --progress --colors --watch",
    "minify": "webpack --progress --colors -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linkeddata/dokieli"
  },
  "keywords": [
    "decentralized",
    "linked data",
    "rdf",
    "research",
    "web annotation",
    "webid"
  ],
  "author": "the dokieli team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/linkeddata/dokieli/issues"
  },
  "homepage": "https://dokie.li/",
  "dependencies": {
    "fs": "0.0.1-security",
    "medium-editor": "^5.23.3",
    "medium-editor-tables": "^0.6.1",
    "node-fetch": "^1.7.3",
    "npm": "^5.5.1",
    "rdf-parser-rdfa": "git://github.com/rdf-ext/rdf-parser-rdfa.git#master",
    "rdf-store-ldp": "^0.3.1",
    "simplerdf": "^0.2.14",
    "simplerdf-parse": "^0.1.3",
    "solid-auth-client": "^2.2.11",
    "trackjs": "^2.13.0",
    "webpack-core": "^0.6.9",
    "wrapper-webpack-plugin": "^2.0.0"
  },
  "devDependencies": {
    "standard": "^10.0.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "standard": {
    "globals": [
      "DO",
      "SimpleRDF",
      "ld",
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
